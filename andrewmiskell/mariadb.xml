<?xml version="1.0"?>
<Container version="2">
  <Name>mariadb</Name>
  <Repository>linuxserver/mariadb</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/mariadb/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://mariadb.org/</Project>
  <Overview>An Enhanced drop in replacement for Mysql</Overview>
  <Category>Network:Other MediaApp:Other Other: Tools:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/andrewmiskell/unraid-docker-templates/main/andrewmiskell/mariadb.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/andrewmiskell/unraid-docker-templates/main/img/mariadb-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1605063912</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>An Enhanced drop in replacement for MySQL</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/mariadb</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MYSQL_ROOT_PASSWORD</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="UserID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="GroupID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="MySQL Root Password" Target="MYSQL_ROOT_PASSWORD" Default="0" Mode="" Description="Container Variable: MYSQL_ROOT_PASSWORD" Type="Variable" Display="always" Required="false" Mask="true"></Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/mariadb" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/mariadb</Config>
</Container>
